&lt;noscript&gt;&lt;pre&gt;def solution(N,number):
    possible_set = [0, [N]]
    if N == number : #
        return 1
    for i in range(2,9):                            #2~8ê¹Œì§€ê°€ ì¡°í•©(9ë²ˆì´ìƒ ì“°ì§€ ì•ŠìŒ)
        case_set = []                               # Possible setì— appendí•  caseset
        basic_num = int(str(N)*i)                   # Nì˜ iê°œ ì—°ì†í•œ ìˆ«ì ì¶”ê°€ ex) 33, 333 , ...
        case_set.append(basic_num)
        for i_half in range(1,i//2+1):              #ìˆ«ìë¥¼ ì ˆë°˜ê¹Œì§€ë§Œ ì‚¬ìš©(ì¢Œìš°ëŒ€ì¹­ìœ¼ë¡œ ê°™ì€ ê²°ê³¼ê°€ ë‚˜ì˜´)
            for x in possible_set[i_half]:
                for y in possible_set[i-i_half]:    # x + y = i_half ì´ë©° ë°˜ìœ¼ë¡œë‚˜ëˆ ì§„
                    print(possible_set)
                                                    # ì‚¬ì¹™ì—°ì‚°ì„ ìˆ˜í–‰í›„ case_setì— ë„£ìŒ
                    case_set.append(x+y)
                    case_set.append(x-y)
                    case_set.append(y-x)
                    case_set.append(x*y)
                    if y != 0:
                        case_set.append(x / y)
                    if x != 0:
                        case_set.append(y / x)
            if number in case_set:                   # case setì— ì›í•˜ëŠ” ìˆ«ìê°€ ìˆë‹¤ë©´ ë°˜í™˜
                return i
            possible_set.append(case_set)
    return -1


